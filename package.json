{
  "name": "@kaivanwong/opuntia",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "simple-git-hooks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.2",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.55.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "simple-git-hooks": "^2.9.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "vite": "^5.0.6",
    "vite-plugin-vue-devtools": "1.0.0-rc.7",
    "vitest": "^1.0.2",
    "vue-tsc": "^1.8.25"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
